<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Pagination?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <ScrollPane prefHeight="107.0" prefWidth="640.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <content>
                  <Text fx:id="testTitle" strokeType="OUTSIDE" strokeWidth="0.0" text="testTitle" />
               </content>
            </ScrollPane>
         </children>
    </AnchorPane>
      <VBox prefHeight="392.0" prefWidth="640.0" spacing="5.0">
         <children>
            <Pagination fx:id="pagination" prefHeight="272.0" prefWidth="640.0">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin></Pagination>
            <Button fx:id="finishButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onFinishButton" text="Завершить тестирование" />
         </children>
      </VBox>
  </children>
</VBox>
